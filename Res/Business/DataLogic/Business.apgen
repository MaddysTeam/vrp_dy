					<add name="ExtName"  type="string" dataLength="20" display="后缀名称"  required="true"/>
					<add name="FilePath" type="string" dataLength="255" display="作品路径" required="true"/>
					<add name="FileSize" type="long" display="文件大小" required="true"/>
