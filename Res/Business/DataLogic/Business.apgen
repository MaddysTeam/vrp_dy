			<!-- 以下为微课程习题表  -->
			<table name="Exercises">
				<columns>
					<add name="ExerciseId" type="long" identityType="Provider" primaryKey="true"/>
					<add name="CourseId" type="long" required="true"/>
					<add name="Name" type="string" dataLength="1000" required="true"></add>
